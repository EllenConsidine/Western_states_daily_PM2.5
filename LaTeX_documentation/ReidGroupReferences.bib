@article{li_discovering_2015,
	title = {Discovering susceptibility genes for allergic rhinitis and allergy using a genome-wide association study strategy},
	volume = {15},
	issn = {1528-4050},
	url = {https://journals.lww.com/co-allergy/Abstract/2015/02000/Discovering_susceptibility_genes_for_allergic.6.aspx},
	doi = {10.1097/ACI.0000000000000124},
	abstract = {Purpose of review Allergic rhinitis and allergy are complex conditions, in which both genetic and environmental factors contribute to the pathogenesis. Genome-wide association studies (GWASs) employing common single-nucleotide polymorphisms have accelerated the search for novel and interesting genes, and also confirmed the role of some previously described genes which may be involved in the cause of allergic rhinitis and allergy. The aim of this review is to provide an overview of the genetic basis of allergic rhinitis and the associated allergic phenotypes, with particular focus on GWASs.
        Recent findings The last decade has been marked by the publication of more than 20 GWASs of allergic rhinitis and the associated allergic phenotypes. Allergic diseases and traits have been shown to share a large number of genetic susceptibility loci, of which IL33/IL1RL1, IL-13-RAD50 and C11orf30/LRRC32 appear to be important for more than two allergic phenotypes. GWASs have further reflected the genetic heterogeneity underlying allergic phenotypes.
        Summary Large-scale genome-wide association strategies are underway to discover new susceptibility variants for allergic rhinitis and allergic phenotypes. Characterization of the underlying genetics provides us with an insight into the potential targets for future studies and the corresponding interventions.},
	language = {en-US},
	number = {1},
	urldate = {2018-01-17},
	journal = {Current Opinion in Allergy and Clinical Immunology},
	author = {Li, Jingyun and Zhang, Yuan and Zhang, Luo},
	month = feb,
	year = {2015},
	pages = {33},
