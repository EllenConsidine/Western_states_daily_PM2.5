	keywords = {Fine particulate matter, exposure, wildfire, particulate matter, Epidemiology, Public health, outcomes, Blended models, Forest fire smoke, air-quality, respiratory health, canada, system, beetle, british-columbia, Exposure   assessment},
	pages = {308--317},
	annotate = {},
