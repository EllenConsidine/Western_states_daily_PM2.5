	pmcid = {PMC5020109},
	pages = {2963--2971},
	file = {PubMed Central Full Text PDF:C\:\\Users\\mema2636\\Zotero\\storage\\D7K38KFX\\Opstelten et al. - 2016 - Exposure to Ambient Air Pollution and the Risk of .pdf:application/pdf}
