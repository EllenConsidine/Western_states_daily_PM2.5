@article{SLEIMAN2015,
title = "The genetic basis of eosinophilic esophagitis",
journal = "Best Practice \& Research Clinical Gastroenterology",
volume = "29",
number = "5",
pages = "701 - 707",
year = "2015",
note = "Eosinophilic esophagitis",
issn = "1521-6918",
doi = "https://doi.org/10.1016/j.bpg.2015.09.003",
url = "http://www.sciencedirect.com/science/article/pii/S152169181500116X",
author = "Patrick M.A. Sleiman and Michael March and Hakon Hakonarson",
keywords = "GWAS, Genetics, EoE, Eosinophilic esophagitis, TSLP, STAT6, CAPN14, ANKRD27, c11orf30",
abstract = "Abstract Eosinophilic esophagitis is characterized by destructive responses of the immune system to environmental allergens, including food, on the human esophagus. EoE is now reported as a major cause of upper gastrointestinal morbidity in children and adults and the incidence is reported to be on the increase. It is known that EoE has a high degree of heritability, with a majority of the phenotypic variation believed to be genetic in origin as shown by genetic epidemiology studies of twins and families. Prior to 2010, there were no known genetic risk factors for the disease. Three GWAS have since been published identifying 5 loci which influence risk for EoE in both children and adults. The information gained from GWAS has been of value in elucidating the pathways involved, such as IL4/STAT6, and more unexpected pathways such as epithelial apical transport and wound healing. We will review the results of the EoE GWAS and the known associated genes, concluding with a discussion of some future directions for genetic studies in EoE."
}

