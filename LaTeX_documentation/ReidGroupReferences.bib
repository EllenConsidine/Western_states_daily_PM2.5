	pages = {1266--71},
	file = {Henderson_etal_2011_Three MethodsExposure_BCFires:C\:\\Users\\Colleen Reid\\Zotero\\storage\\3NZUA4H3\\Henderson_etal_2011_Three MethodsExposure_BCFires.pdf:application/pdf;Henderson_etal_2011_Three MethodsExposure_BCFires:C\:\\Users\\Colleen Reid\\Zotero\\storage\\SZP7J5T4\\Henderson_etal_2011_Three MethodsExposure_BCFires.pdf:application/pdf}
}


