@article{haikerwal_fine_2016,
	title = {Fine particulate matter ({PM}\textsubscript{2.5} ) exposure during a prolonged wildfire period and emergency department visits for asthma},
	volume = {21},
	issn = {1440-1843},
	doi = {10.1111/resp.12613},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/resp.12613},
	abstract = {BACKGROUND AND OBJECTIVE: The 2006-2007 wildfire period was one of the most extensive and long lasting fires in Australian history with high levels of fine particulate matter (PM2.5 ). Large populations were exposed to smoke for over 2 months. The study aimed to investigate the association between wildfire-related PM2.5 exposure and emergency department (ED) visits for asthma.
METHODS: A time-stratified case-crossover design was used to investigate associations between daily average PM2.5 and ED attendances for asthma from December 2006 to January 2007. ED data were obtained from the Victorian Emergency Minimum Dataset. Smoke dispersion during the wildfire event was modelled using a validated chemical transport model. Exposure data (daily average PM2.5 , temperature and relative humidity) were modelled for the study period. Various lag periods were investigated.
RESULTS: There were 2047 ED attendances for asthma during the study period. After adjusting for temperature and relative humidity, an interquartile range increase in PM2.5 levels of 8.6 μg/m(3) was associated with an increase in ED attendances for asthma by 1.96\% (95\%CI: 0.02, 3.94) on the day of exposure. Lag periods up to 2 days prior did not show any association. A strong association was observed among women 20 years and older (5.08\% 95\%CI: 1.76, 8.51).
CONCLUSIONS: Wildfire-related PM2.5 was associated with increased risk of ED attendance for asthma during the wildfire event. It is important to understand the role of wildfire PM2.5 as a trigger for asthma presentations.},
	language = {eng},
	number = {1},
	journal = {Respirology (Carlton, Vic.)},
	author = {Haikerwal, Anjali and Akram, Muhammad and Sim, Malcolm R. and Meyer, Mick and Abramson, Michael J. and Dennekamp, Martine},
	year = {2016},
	pmid = {26346113},
	keywords = {Asthma, Particulate Matter, Female, Humans, Adult, Young Adult, Risk Factors, Fires, Age Factors, smoke, Inhalation Exposure, Australia, Air Pollutants, Emergency Service, Hospital, Cross-Over Studies, Sex Factors, asthma, emergency services/hospitals, Smoke, particulate matter, fires},
	pages = {88--94},
	annotate = {This study used case-crossover design to analyze ED visits for asthma with PM\textsubscript{2.5} during a prolonged wildfire period and stratified by age and sex.},
	AnnSymbol = {{* }},
}
