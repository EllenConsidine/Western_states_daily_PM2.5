	keywords = {Ultrafine particles, lung-cancer, particulate matter, Human health, air-liquid interface, alveolar   macrophages, beas-2b, bioconductor package, Biomass particles, clinical-implications, combustion emissions, Diesel particles, Environmental   particles, exhaust particles, Gene network, in-vitro, Lung   disorders, oxidative   stress, RNA-seq, Time-course},
	pages = {302},
