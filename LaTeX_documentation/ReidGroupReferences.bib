	journal = {International Journal of Psychiatry in Clinical Practice},
	author = {Psarros, Constantin and Theleritis, Christos and Economou, Marina and Tzavara, Chara and Kioulos, Kanellos T. and Mantonakis, Leonidas and Soldatos, Constantin R. and Bergiannaki, Joanna-Despoina},
