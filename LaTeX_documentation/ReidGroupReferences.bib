	keywords = {Adult, African Americans, Coronary Disease, Demography, European Continental Ancestry Group, Female, Follow-Up Studies, Humans, Male, Odds Ratio, Prevalence, Risk Factors, Smoking, Social Class, Social Environment, Socioeconomic Factors, United States},
	pages = {315--326}
}


